    "node-cron": "^3.0.3",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0",
